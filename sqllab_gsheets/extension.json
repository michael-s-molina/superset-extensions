{
  "id": "sqllab_gsheets",
  "name": "Export SQL Lab to Google Sheets",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "frontend": {
    "contributions": {
      "commands": [
        {
          "command": "sqllab_gsheets.export",
          "title": "Export to Google Sheets"
        }
      ],
      "menus": {
        "sqllab": {
          "editor": {
            "secondary": [
              {
                "view": "builtin.editor",
                "command": "sqllab_gsheets.export"
              }
            ]
          }
        }
      }
    },
    "moduleFederation": {
      "exposes": ["./index"]
    }
  },
  "backend": {
    "entryPoints": ["sqllab_gsheets.entrypoint"],
    "files": ["backend/src/sqllab_gsheets/**/*.py"]
  },
  "permissions": ["can_read", "can_view"]
}
